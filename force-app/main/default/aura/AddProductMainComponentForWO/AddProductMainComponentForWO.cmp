<aura:component
  controller="AddProductMainCompWoController"
  implements="force:lightningQuickActionWithoutHeader,force:appHostable,lightning:actionOverride,flexipage:availableForAllPageTypes,flexipage:availableForRecordHome,force:hasRecordId,forceCommunity:availableForAllPageTypes"
  access="global"
>
  <aura:attribute
    name="recordFrom"
    type="String"
    access="public"
  ></aura:attribute>
  <aura:attribute
    name="workOrderId"
    type="String"
    access="public"
  ></aura:attribute>
  <aura:attribute
    name="opportunityId"
    type="String"
    access="public"
  ></aura:attribute>
  <aura:attribute name="recordId" type="String"></aura:attribute>

  <aura:handler name="init" value="{!this}" action="{!c.doInit}"></aura:handler>
  <ui:button aura:id="backButton" label="Back" press="{!c.backAction}" />

  <div class="slds-box slds-theme_default">
    <lightning:tabset selectedTabId="one">
      <lightning:tab label="ESP" id="one">
        <c:AddProductESPTab
          recordId="{!v.workOrderId}"
          recordFrom="{!v.recordFrom}"
        />
      </lightning:tab>
      <lightning:tab label="Team Phun" id="two">
        <!-- <c:AddProductTeamPhunTabForWO tabname="teamphun" recordId="{!v.workOrderId}" recordFrom="{!v.recordFrom}"/> -->
        <c:AddProductTeamPhunTab
          tabname="teamphun"
          recordId="{!v.workOrderId}"
          recordFrom="{!v.recordFrom}"
        />
      </lightning:tab>
      <lightning:tab label="Design Products" id="three">
        <c:AddProductTeamPhunTab
          tabname="designproduct"
          recordId="{!v.workOrderId}"
          recordFrom="{!v.recordFrom}"
        />
      </lightning:tab>
    </lightning:tabset>
  </div>
</aura:component>
